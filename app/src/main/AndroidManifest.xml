<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.emr">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light">
        <activity android:name=".Historico.CalendarioActivity"></activity>
        <activity android:name=".Slide.Ultimo" />
        <activity android:name=".Slide.Calendar" />
        <activity android:name=".AgendarActivity2" />
        <activity android:name=".Slide.Agendar" />
        <activity android:name=".MenuActivity" />
        <activity
            android:name=".RecuperarActivity"
            android:label="@string/titulorecuperar"
            android:parentActivityName=".LoginActivity" />
        <activity
            android:name=".CadastrarActivity"
            android:label="@string/titulo_cadastrar"
            android:parentActivityName=".LoginActivity" />
        <activity
            android:name=".LoginActivity"
            android:label="@string/titulo_login"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.Intro">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>